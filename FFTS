    Name = "auto farm ",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
}) 

AutoAttackTab:AddToggle({
    Name = "Enable Auto farm WARP FRUIT (Every 3s)",
    Default = false,
    Callback = function(value)
        autoAttackEnabled = value
        if value then
            toggleAutoAttack()
        end
    end
})

